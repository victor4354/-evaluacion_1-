#include <stdio.h>
void sumaMatriz(int t,int a[]10,int b[]10),int c[]10)
{
     for (int i=0,i<t,i++)
     {
     
     for (int j=0;j<t;j++)
     {
     c[i][j]=a[i][j]+b[i][j];
     printf("%d",c[i][j];
     }
    }
}
